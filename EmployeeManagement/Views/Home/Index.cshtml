@model IEnumerable<EmployeeManagement.Models.Emploeey>
<!DOCTYPE html>
<html>
    <head>
        <title></title>

    </head>
    <body>
        <table>
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Department</th>
                    <th>Emil</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var employee in Model){
                    <tr>
                    <td>@employee.Id</td>
                    <td>@employee.Name</td>
                    <td>@employee.Department</td>
                    <td>@employee.Emil</td>

                    </tr>
                }
            </tbody>
        </table>
    </body>
</html>