@model IEnumerable<Emploeey>
@{
    ViewBag.Title = "Employee List";
}
    <div class="row card-deck">
        @foreach(var employee in Model){
            <div class="col-3 card m-3">
                <div class="card-header">
                    <h3>@employee.Name</h3>
                </div>
                <img class="card-img-top imageThumbnail" src="~/images/noimage.jpg"asp-append-version="true"/>
                <div class="card-footer text-center">
                    <a asp-action="Detalis" asp-controller="Home"asp-route-id="@employee.Id" class="btn btn-primary">View</a>
                    <a href="#" class="btn btn-primary">Edit</a>
                    <a href="#" class="btn btn-danger">Delete</a>

            </div>
        </div>

        }
    </div>
    @section Scripts {
    <script src="~/js/site.js"></script>
    }